{

  "manifest_version": 2,

  "name": "Webdm Extension",
  "description": "Start of a webdm extension",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Webdm"
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "permissions": [
  ],

  "content_scripts": [

    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }

  ]
  
}